{
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-bitwise": "warn",
        "import/extensions": ["error", "ignorePackages", {
            "js": "always",
            "mjs": "never",
            "jsx": "never"
          }]
    }
}
